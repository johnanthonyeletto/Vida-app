{
  "expo": {
    "name": "Vida",
    "description": "An app for life coaches to manage their clients.",
    "slug": "vida",
    "privacy": "unlisted",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios"
    ],
    "version": "1.0",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vida.vida"
    }
  }
}